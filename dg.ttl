@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix nidm: <http://purl.org/nidash/nidm#> .

nidm:AcquisitionObject rdf:type owl:Class .
nidm:AcquisitionObject rdfs:label "Acquisition Object"^^xsd:string .
nidm:AcquisitionObject rdfs:subClassOf prov:Entity .
nidm:AcquisitionObject nidm:definition "An acquisition object is an entity that represents a single stream of data, where multiple acquisitions in the same study take place simultaneously. For example, fMRI and physiological recordings." .

###  http://purl.org/nidash/nidm#DemographicsAcquisitionObject
nidm:DemographicsAcquisitionObject rdf:type owl:Class ;
  rdfs:label "Demographics Acquisition Object"^^xsd:string ;
  rdfs:subClassOf nidm:AcquisitionObject ;
  nidm:definition "A demographics acquisition object is an entity that represents the demographics information acquired from a person in the subject role." .
